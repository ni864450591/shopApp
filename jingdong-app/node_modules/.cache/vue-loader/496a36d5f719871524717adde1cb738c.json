n { item, getItemdata }\r\n}\r\n\r\nexport default {\r\n  name: 'Shop',\r\n  components: { ShopInfo, Content, Cart, Search },\r\n  setup () {\r\n    const { item, getItemdata } = useShopItemEffect()\r\n    getItemdata()\r\n    return { item }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.wrapper{\r\n  padding:0 .18rem\r\n}\r\n</style>\r\n"]}]}